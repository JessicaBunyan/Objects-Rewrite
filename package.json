  "private": true,
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "build-css": "less-watch-compiler --run-once src/less src/css",
