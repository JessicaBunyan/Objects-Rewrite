    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "jest-html": "jest-html"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "jest-html": "^1.5.0",
    "less-watch-compiler": "^1.13.0",
    "react-test-renderer": "^16.8.2"
  }
